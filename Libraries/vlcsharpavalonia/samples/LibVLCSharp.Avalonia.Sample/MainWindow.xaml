<Window
    x:Class="LibVLCSharp.Avalonia.Sample.MainWindow"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:v="clr-namespace:LibVLCSharp.Avalonia;assembly=LibVLCSharp.Avalonia"
    Title="LibVLCSharp.Avalonia.Sample"
    Width="800"
    Height="450"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <StackPanel
        HorizontalAlignment="Center"
        VerticalAlignment="Center"
        Spacing="20">
        <Button
            x:Name="example1"
            Width="100"
            Content="Example 1" />
        <Button
            x:Name="example2"
            Width="100"
            Content="Example 2" />
    </StackPanel>
</Window>
