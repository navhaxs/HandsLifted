<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="200"
        Width="400" Height="200"
        x:Class="HandsLiftedApp.Importer.GoogleSlides.ImportGoogleSlidesWindow"
        Topmost="True"
        Title="Import Google Slides">
    <DockPanel>
        <StackPanel DockPanel.Dock="Bottom" HorizontalAlignment="Right" Margin="12">
            <Button Click="Button_OnClick" IsDefault="True">Import</Button>
        </StackPanel>

        <StackPanel Margin="12">
            <TextBlock>Google Slides Presentation ID:</TextBlock>
            <TextBox Text="{Binding ImportId}" />
        </StackPanel>
    </DockPanel>
</Window>