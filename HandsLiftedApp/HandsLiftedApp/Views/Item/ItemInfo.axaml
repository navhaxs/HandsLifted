<UserControl xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:ext="using:HandsLiftedApp.Extensions"
			 xmlns:utils="using:HandsLiftedApp.Utils"
			 xmlns:views="using:HandsLiftedApp.Views"
			 xmlns:viewsrender="using:HandsLiftedApp.Views.Render"
			 xmlns:hla="using:HandsLiftedApp.XTransitioningContentControl"
			 xmlns:controls="using:HandsLiftedApp.Controls"
 			 xmlns:item="using:HandsLiftedApp.Views.Item"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 x:Class="HandsLiftedApp.Views.Item.ItemInfo">
	<ContentControl Content="{Binding}" >
		<ContentControl.ContentTemplate>
			<utils:MyTemplateSelector>
				<DataTemplate x:Key="SongItem">
				</DataTemplate>
				<DataTemplate x:Key="GoogleSlidesGroupItem">
					<item:GoogleSlidesInfo VerticalAlignment="Top" />
				</DataTemplate>
				<DataTemplate x:Key="PowerPointSlidesGroupItem">
					<item:PowerPointInfo VerticalAlignment="Top" />
				</DataTemplate>
				<DataTemplate x:Key="Fallback">
					<Border Height="40" Background="Red" />
				</DataTemplate>
			</utils:MyTemplateSelector>
		</ContentControl.ContentTemplate>
		<!-- NOTE: do *NOT* use ContentControl.DataTemplates or it will bubble down -->
		<!--<ContentControl.DataTemplates>
		</ContentControl.DataTemplates>-->
	</ContentControl>


</UserControl>
