<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:HandsLiftedApp"
             x:Class="HandsLiftedApp.App">
	<Application.DataTemplates>
		<local:ViewLocator/>
	</Application.DataTemplates>

	<Application.Styles>
		<StyleInclude Source="avares://Avalonia.Themes.Default/DefaultTheme.xaml" />
		<StyleInclude Source="avares://Avalonia.Themes.Default/Accents/BaseLight.xaml" />

		<!--<FluentTheme Mode="Light"/>-->
		<StyleInclude Source="avares://LibVLCSharp.Avalonia/Styles.xaml" />
		<StyleInclude Source="avares://HandsLiftedApp.XTransitioningContentControl/Styles.xaml" />
		<StyleInclude Source="avares://HandsLiftedApp/Styles/Styles1.axaml" />
		<StyleInclude Source="avares://HandsLiftedApp/Icons.axaml" />
		<StyleInclude Source="avares://Material.Icons.Avalonia/App.xaml"></StyleInclude>
	</Application.Styles>
</Application>
